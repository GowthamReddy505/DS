class Main 
{
    public static void main(String[] args)
    
    {
        int arr[]={12,34,5,6,23};
        
        for(int i=0;i<arr.length;i++)
        {
            if(i%2==0)
            {
               System.out.println("even position"+arr[i]);
            }                
        }
        
        
    }
}
